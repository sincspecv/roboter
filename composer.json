{
  "name": "the-fancy-robot/roboter",
  "description": "Boilerplate WordPress project",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Matthew Schroeter",
      "email": "matt@thefancyrobot.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "sincspecv/wp-acf-to-post": "0.1.7",
    "tfr/advanced-custom-fields-pro": "6.0.5",
    "tfr/gravityforms": "2.6.8",
    "roots/soil": "^4.1.1",
    "wpackagist-plugin/seo-by-rank-math": "dev-trunk"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:sincspecv/WP-ACF-To-Post.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:sincspecv/gravityforms.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:sincspecv/advanced-custom-fields-pro.git"
    }
  ],
  "scripts": {
    "post-install-cmd": "bash post-install.sh"
  },
  "post-autoload-dump": [
    "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
